{
    "name": "tab-manager-next",
    "displayName": "Tab Manager Next",
    "version": "0.0.1",
    "description": "Manage your tabs with AI",
    "author": "Shiqi Mei <shiqi.mei@outlook.com>",
    "contributors": [
        "Shiqi Mei <shiqi.mei@outlook.com>"
    ],
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package"
    },
    "dependencies": {
        "plasmo": "^0.89.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sharp": "0.32.6"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.1.1",
        "@types/chrome": "0.0.258",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "prettier": "3.2.4",
        "typescript": "5.3.3"
    },
    "manifest": {
        "host_permissions": [
            "https://*/*"
        ],
        "permissions": [
            "scripting"
        ],
        "web_accessible_resources": [
            {
                "matches": [
                    "<all_urls>"
                ],
                "resources": [
                    "tabs/delta-flyer.html"
                ]
            }
        ]
    }
}